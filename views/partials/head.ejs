<meta charset='utf-8'>
<meta name='viewport' content='width=device-width'>

<link rel='preload' href='/fonts/normal.woff2' as='font'>
<link rel='preload' href='/fonts/bold.woff2' as='font'>
<link rel='stylesheet' href='/style.css' type='text/css'>

<link rel='icon' href='/favicon.ico'>
<link rel='icon' href='/favicon.png'>
<link rel='shortcut icon' href='/favicon.ico'>
<link rel='shortcut icon' href='/favicon.png'>

<% if (typeof name !== 'undefined') { %>
  <title><%= name %> | Arachnid</title>
<% } else { %>
  <title>Arachnid</title>
<% } %>

<link rel='canonical' href='https://arachnid.cc<%= canonical %>'>

<% if (typeof name !== 'undefined') { %>
  <meta name='title' content='<%= name %> | Arachnid'>
<% } else { %>
  <meta name='title' content='Arachnid'>
<% } %>
<meta name='description' content='A virtual and scalable network based on existing standards like TCP/IP. Allocate large subnets and custom TLDs for free in the open-source and secure web, Arachnid.'>
<meta name='theme-color' content='#9e39ee'>

<meta property='og:type' content='website'>
<meta property='og:site_name' content='Arachnid'>
<meta property='og:canonical' content='https://arachnid.cc<%= canonical %>'>
<% if (typeof name !== 'undefined') { %>
  <meta property='og:title' content='<%= name %>'>
<% } else { %>
  <meta property='og:title' content='Arachnid'>
<% } %>
<meta property='og:description' content='A virtual and scalable network based on existing standards like TCP/IP. Allocate large subnets and custom TLDs for free in the open-source and secure web, Arachnid.'
<meta property='og:image' content='/graphics/banner.png'>

<meta property='twitter:card' content='summary_large_image'>
<meta property='twitter:canonical' content='https://arachnid.cc<%= canonical %>'>
<% if (typeof name !== 'undefined') { %>
  <meta property='twitter:title' content='<%= name %> | Arachnid'>
<% } else { %>
  <meta property='twitter:title' content='Arachnid'>
<% } %>
<meta property='twitter:description' content='A virtual and scalable network based on existing standards like TCP/IP. Allocate large subnets and custom TLDs for free in the open-source and secure web, Arachnid.'>
<meta property='twitter:image' content='/graphics/banner.png'>